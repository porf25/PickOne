<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.porf.pickone" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/skydiving"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChooseOptions"
            android:label="@string/title_activity_choose_options" >
        </activity>
        <activity
            android:name=".SeeList"
            android:label="@string/title_activity_see_list" >
        </activity>
        <activity
            android:name=".Results"
            android:label="@string/title_activity_results" >
        </activity>
    </application>

</manifest>
